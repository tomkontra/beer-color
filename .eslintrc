{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
        "experimentalObjectRestSpread": true
    }
},
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "semi": ["error", "never"],
    "semi-style": ["error", "first"],
    "prettier/prettier": ["error"]
  }
}
